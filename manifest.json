{
    "_metadata": {
        "major_version": 1,
        "minor_version": 1
    },
    "display_information": {
        "name": "Groot",
        "description": "At your service",
        "background_color": "#615450"
    },
    "features": {
        "app_home": {
            "home_tab_enabled": false,
            "messages_tab_enabled": true,
            "messages_tab_read_only_enabled": true
        },
        "bot_user": {
            "display_name": "Groot",
            "always_online": false
        },
        "slash_commands": [
            {
                "command": "/createaccount",
                "url": "https://jadepalace-server.aws.prodigaltech.com/slack/commands/createAccount",
                "description": "creates account",
                "should_escape": false
            },
            {
                "command": "/createsingleaccount",
                "url": "https://jadepalace-server.aws.prodigaltech.com/slack/commands/createSingleAccount",
                "description": "createsingleaccount ProVoice/ProNotes will create single account ",
                "usage_hint": "Use \"/createSingleAccount ProNotes\"      for creating ProNotes account",
                "should_escape": false
            }
        ]
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "app_mentions:read",
                "chat:write",
                "commands",
                "incoming-webhook"
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "request_url": "https://jadepalace-server.aws.prodigaltech.com/slack/events",
            "bot_events": [
                "app_mention"
            ]
        },
        "interactivity": {
            "is_enabled": true,
            "request_url": "https://jadepalace-server.aws.prodigaltech.com/slack/payloads"
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": false,
        "token_rotation_enabled": false
    }
}